{
  "name": "lems",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --port 8080",
    "start": "nodemon --watch server/src --exec ts-node --swc server/src/index.ts",
    "lint": "vue-cli-service lint",
    "build:client": "vue-cli-service build",
    "build:server": "tsc --project server/tsconfig.json",
    "build": "yarn build:client && yarn build:server",
    "test:e2e": "vue-cli-service test:e2e",
    "test:server": "jest -c server/jest.config.js --runInBand",
    "test:unit": "vue-cli-service test:unit",
    "test": "vue-cli-service test:unit && vue-cli-service test:e2e --headless && jest -c server/jest.config.js",
    "deploy": "node server/build/server/src/index.js",
    "heroku-logs-staging": "heroku logs --tail -a lems-staging"
  },
